{
    "semi": true,
    "singleQuote": false,
    "tabWidth": 4,
    "trailingComma": "all",
    "printWidth": 180,
    "arrowParens": "always",
    "bracketSpacing": true,
    "endOfLine": "lf",
    "useTabs": false,
    "quoteProps": "as-needed",
    "proseWrap": "preserve",
    "htmlWhitespaceSensitivity": "ignore",
    "embeddedLanguageFormatting": "auto",
    "plugins": ["prettier-plugin-organize-imports"]
}
