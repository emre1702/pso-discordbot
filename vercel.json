{
    "$schema": "https://openapi.vercel.sh/vercel.json",
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "backend/dist/main.js"
        }
    ],
    "buildCommand": "npm run build",
    "installCommand": "npm run install:all",
    "outputDirectory": "backend/dist"
}
